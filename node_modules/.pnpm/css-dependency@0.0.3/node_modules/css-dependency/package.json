{
  "name": "css-dependency",
  "description": "A css parser for getting imports from css code.",
  "version": "0.0.3",
  "license": "MIT",
  "author": {
    "name": "Aidan Bleser",
    "url": "https://github.com/ieedan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ieedan/css-dependency"
  },
  "bugs": {
    "url": "https://github.com/ieedan/css-dependency/issues"
  },
  "type": "module",
  "keywords": [
    "css"
  ],
  "files": [
    "dist/**/*",
    "src/**/*",
    "package.json",
    "pnpm-lock.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.8",
    "@types/node": "^22.13.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "ansi-regex": "^6.1.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check && tsc --noEmit",
    "test": "vitest",
    "changeset": "changeset",
    "ci:release": "pnpm build && changeset publish"
  }
}